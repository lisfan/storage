{
  "name": "@~lisfan/storage",
  "version": "1.0.0-rc.5",
  "description": "离线存储器",
  "main": "src/index.js",
  "scripts": {
    "pub-pre": "git pull && npm update && npm version prerelease && git add . && git commit -m 'updata: update version' && git push",
    "pub-n": "npm run pub-pre && npm publish --tag nightly",
    "pub-a": "npm run pub-pre && npm publish --tag alpha",
    "pub-b": "npm run pub-pre && npm publish --tag beta",
    "pub-rc": "npm run pub-pre && npm publish --tag rc",
    "pub-s": "npm run pub-pre && npm publish --tag stable",
    "pub": "npm run pub-pre && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "rm -rf docs && jsdoc -c conf/jsdoc.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisfan/timer.git"
  },
  "keywords": [
    "sessionStorage",
    "localStorage",
    "webSQL",
    "indexedDB",
    "offline storage",
    "storage",
    "store"
  ],
  "author": "lisfan <goolisfan@gmail.com> (https://www.npmjs.com/~lisfan)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lisfan/storage/issues"
  },
  "homepage": "https://github.com/lisfan/storage#readme",
  "dependencies": {
    "@~lisfan/logger": "^1.2.0-rc.1",
    "@~lisfan/validation": "^1.0.0-rc.9"
  },
  "peerDependencies": {
    "localforage": "^1.4.3"
  },
  "devDependencies": {
    "docdash": "^0.4.0",
    "jsdoc": "^3.5.5"
  }
}
